<%= form_for [ user, user.restaurants.new ], remote: true do |f| %>
  <div class='field add'>
    <%= f.label :Add %>
    <%= f.text_field :title, class: 'field', placeholder: 'Add a new restaurant.' %></br>
  </div>
  <div class='field rate'>
    <%= f.label :rating %>
    <%= f.number_field :rating %>
  </div>
  <div class="actions">
    <%= f.submit 'Save', class: 'btn btn-success' %>
  </div>
<% end %>
